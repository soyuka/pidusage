    "mockery": "^2.1.0",
    "nyc": "^15.1.0",
    "pify": "^3.0.0",
    "standard": "^16.0.4",
    "string-to-stream": "^1.1.1",
    "through": "^2.3.8",
    "time-span": "^2.0.0"
